
public class Idk
{

	public static void main(String[] args)
	{
		// TODO Auto-generated method stub
		double getalA;
		getalA = 10;
		double getalB = 2;
		double antwoord = getalA * getalB;
		System.out.print("Antwoord Vermenigvuldiging: ");
		System.out.println(antwoord);
		
		double getalC;
		getalC = 10;
		double getalD = 2;
		double antwoord1 = getalC / getalD;
		System.out.print("Antwoord Delen: ");
		System.out.println(antwoord1);
		
		double getalE;
		getalE = 10;
		double getalF = 2;
		double antwoord2 = getalE - getalF;
		System.out.print("Antwoord Vermenigvuldiging: ");
		System.out.println(antwoord2);
	}

}
